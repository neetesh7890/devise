<div class="">
  <h1> Welcome <%= current_user.name %></h1>
  <%= image_tag current_user_avatar %>
</div>

<%= link_to "Gmail contacts", "/dashboards/gmail" %>

<div>
  <% @contacts.each do |item| %>
    <div class="col-md-6">
      <h3> Name: </h3> <%= item[:first_name] %>
    </div>

    <div class="col-md-6">
      <% item[:emails].each do |all_email| %>
        <h3> Email: </h3> <%= all_email[:email] %>
      <% end %>
    </div>
  <% end %>
</div>




